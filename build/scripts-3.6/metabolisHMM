import argparse 
import os
import sys

if sys.version_info[0] < 3:
    print("You are running metabolisHMM with python2 - you need python3.")
    sys.exit(1)

import metabolisHMM.argumentParser
from metabolisHMM.controller import Controller

if __name__=='__main__':
    args = metabolisHMM.argumentParser.parse_args(sys.argv[2:])
    controller = Controller()
    controller.parseArguments(args)
